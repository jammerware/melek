{
  "webroot": "wwwroot",
  "version": "1.0.0-*",

  "dependencies": {
    "Microsoft.AspNet.Mvc": "6.0.0-beta6",
    "Melek": "1.0.13-*",
    "Melek.Client": "1.0.13-*",
    "Kestrel": "1.0.0-beta5"
  },

  "commands": {
    "kestrel": "Microsoft.AspNet.Hosting --server Kestrel --server.urls http://localhost:5004",
    "web": "Microsoft.AspNet.Hosting --config hosting.ini"
  },

  "frameworks": {
    "dotnet": {
      "dependencies": {
        "System.Collections": "4.0.10",
        "System.Linq": "4.0.0",
        "System.Threading": "4.0.10",
        "Microsoft.CSharp": "4.0.0",
      }
    }
  },

  "exclude": [
    "wwwroot",
    "node_modules",
    "bower_components"
  ],
  "publishExclude": [
    "node_modules",
    "bower_components",
    "**.xproj",
    "**.user",
    "**.vspscc"
  ]
}